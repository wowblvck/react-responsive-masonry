{
  "name": "@wowblvck/react-responsive-masonry",
  "version": "2.0.3",
  "author": {
    "name": "CÃ©dric Delpoux",
    "email": "cedric.delpoux@gmail.com"
  },
  "description": "React responsive masonry component built with css flexbox",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "homepage": "https://github.com/cedricdelpoux/react-responsive-masonry#readme",
  "keywords": [
    "react",
    "masonry",
    "css",
    "flexbox",
    "responsive"
  ],
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./Masonry": {
      "import": "./dist/esm/Masonry/index.mjs",
      "require": "./dist/cjs/Masonry/index.js",
      "types": "./dist/types/Masonry/index.d.ts"
    },
    "./ResponsiveMasonry": {
      "import": "./dist/esm/ResponsiveMasonry/index.mjs",
      "require": "./dist/cjs/ResponsiveMasonry/index.js",
      "types": "./dist/types/ResponsiveMasonry/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cedricdelpoux/react-responsive-masonry.git"
  },
  "scripts": {
    "build": "rslib build",
    "lint": "biome check .",
    "format": "biome check . --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@rsbuild/plugin-react": "^1.4.0",
    "@rslib/core": "^0.12.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.9.2"
  }
}
